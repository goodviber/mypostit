<h3>Welcome!</h3>



<table  width=400>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.description %></td>
        <td><%= post.user.username %></td>
        <td><%= link_to 'Show Post', post %></td>
        <td><%= link_to 'Edit Post', edit_post_path(post) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
